<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CarouselViewPager">
        <attr name="interval" format="integer"/>
        <attr name="autoStart" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ProgressView">
        <attr name="arrowEnabled" format="boolean"/>
        <attr name="startTrim" format="float"/>
        <attr name="endTrim" format="float"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ShapeView">
        <attr name="color" format="color"/>
        <attr name="shape" format="enum">
            <enum name="rectangle" value="0"/>
            <enum name="CIRCLE" value="1"/>
            <enum name="TRIANGLE" value="2"/>
        </attr>
        <attr name="rectRadius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ProgressButton">
        <attr name="processing" format="boolean"/>
        <attr name="processingHint" format="string"/>
        <attr name="progressBarColor" format="color"/>
    </declare-styleable>
</resources>